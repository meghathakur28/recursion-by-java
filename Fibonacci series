import java.util.Scanner;
public class abc {
    int printFib(int n){

        if(n==1||n==0)
        {
            return n;

        }
        return printFib(n-1)+printFib(n-2);





    }
    public static void main(String[] args)
    {
         Scanner sc=new Scanner(System.in);
         abc obj=new abc();
         System.out.println("enter the number n: ");
         int n = sc.nextInt();
         for(int i=0;i<=n;i++) {
             System.out.println(obj.printFib(i));
         }
    }
}
